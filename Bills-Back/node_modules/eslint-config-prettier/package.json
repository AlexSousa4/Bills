{
  "name": "eslint-config-prettier",
  "version": "1.1.0",
  "license": "MIT",
  "author": "Simon Lydell",
  "description": "Turns all rules that are unnecessary or might conflict with prettier off.",
  "repository": "lydell/eslint-config-prettier",
  "files": [
    "bin/",
    "flowtype.js",
    "index.js",
    "react.js"
  ],
  "bin": {
    "eslint-config-prettier-check": "bin/cli.js"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "prettier"
  ],
  "scripts": {
    "test:lint": "eslint .",
    "test:lint-verify-fail": "eslint test-lint/ --config .eslintrc.base.js && exit 1 || exit 0",
    "test:lint-rules": "eslint --config test-config/.eslintrc.js --format json index.js",
    "test:ava": "ava",
    "test:cli-sanity": "eslint --print-config .eslintrc.js | ./bin/cli.js",
    "test": "npm run test:lint && npm run test:lint-verify-fail && npm run test:ava && npm run test:cli-sanity"
  },
  "dependencies": {
    "get-stdin": "^5.0.1"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-eslint": "^7.1.1",
    "dedent": "^0.7.0",
    "eslint": "^3.15.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-prettier": "^2.0.0",
    "eslint-plugin-react": "^6.9.0",
    "prettier": "^0.16.0",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "eslint": ">=3.14.1"
  }
}
